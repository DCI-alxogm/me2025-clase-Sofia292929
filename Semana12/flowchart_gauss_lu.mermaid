graph TD
    subgraph GaussSeidel["MÉTODO GAUSS-SEIDEL"]
        GS_Start([Inicio Gauss-Seidel]) --> GS1[Inicializar todas las variables<br/>x[i] = 0 para i = 0 hasta N-1]
        GS1 --> GS2[iter = 0<br/>error = 0]
        GS2 --> GS3[Guardar valores anteriores<br/>x_prev[i] = x[i]]
        GS3 --> GS4[i = 0]
        GS4 --> GS5[suma = 0]
        GS5 --> GS6[j = 0]
        GS6 --> GS7{j != i?}
        GS7 -->|Sí| GS8[suma = suma + A[i][j] × x[j]]
        GS8 --> GS9[j = j + 1]
        GS9 --> GS10{j < N?}
        GS10 -->|Sí| GS7
        GS10 -->|No| GS11[Calcular nuevo valor:<br/>x[i] = A[i][N] - suma / A[i][i]]
        GS11 --> GS12[error = error + |x[i] - x_prev[i]|]
        GS12 --> GS13[i = i + 1]
        GS13 --> GS14{i < N?}
        GS14 -->|Sí| GS5
        GS14 -->|No| GS15[iter = iter + 1]
        GS15 --> GS16{error > TOL<br/>Y<br/>iter < MAX_ITER?}
        GS16 -->|Sí| GS3
        GS16 -->|No| GS17[Imprimir iteraciones realizadas]
        GS17 --> GS18{iter >= MAX_ITER?}
        GS18 -->|Sí| GS19[Mostrar: No converge]
        GS18 -->|No| GS20[Solución encontrada]
        GS19 --> GS_End
        GS20 --> GS_End([Fin Gauss-Seidel])
        GS7 -->|No| GS9
    end

    subgraph FactLU["MÉTODO FACTORIZACIÓN LU"]
        LU_Start([Inicio Factorización LU]) --> LU1[Inicializar matrices<br/>L[MAX][MAX] = 0<br/>U[MAX][MAX] = 0]
        LU1 --> LU2[Extraer vector independiente<br/>b[i] = A[i][N] para i = 0 hasta N-1]
        LU2 --> LU3[i = 0]
        LU3 --> LU4[Calcular fila i de U]
        LU4 --> LU5[k = i hasta N-1]
        LU5 --> LU6[suma = 0]
        LU6 --> LU7[j = 0 hasta i-1]
        LU7 --> LU8[suma = suma + L[i][j] × U[j][k]]
        LU8 --> LU9[U[i][k] = A[i][k] - suma]
        LU9 --> LU10[k = k + 1]
        LU10 --> LU11{k < N?}
        LU11 -->|Sí| LU6
        LU11 -->|No| LU12[Calcular columna i de L]
        LU12 --> LU13[k = i hasta N-1]
        LU13 --> LU14{k == i?}
        LU14 -->|Sí| LU15[L[i][i] = 1]
        LU14 -->|No| LU16[suma = 0]
        LU16 --> LU17[j = 0 hasta i-1]
        LU17 --> LU18[suma = suma + L[k][j] × U[j][i]]
        LU18 --> LU19[L[k][i] = A[k][i] - suma / U[i][i]]
        LU15 --> LU20[k = k + 1]
        LU19 --> LU20
        LU20 --> LU21{k < N?}
        LU21 -->|Sí| LU14
        LU21 -->|No| LU22[i = i + 1]
        LU22 --> LU23{i < N?}
        LU23 -->|Sí| LU4
        LU23 -->|No| LU24[Imprimir matriz L]
        LU24 --> LU25[Imprimir matriz U]
        LU25 --> LU26[Sustitución hacia adelante<br/>Resolver Ly = b]
        LU26 --> LU27[i = 0 hasta N-1]
        LU27 --> LU28[suma = 0]
        LU28 --> LU29[j = 0 hasta i-1]
        LU29 --> LU30[suma = suma + L[i][j] × y[j]]
        LU30 --> LU31[y[i] = b[i] - suma]
        LU31 --> LU32[Sustitución hacia atrás<br/>Resolver Ux = y]
        LU32 --> LU33[i = N-1 hasta 0]
        LU33 --> LU34[suma = 0]
        LU34 --> LU35[j = i+1 hasta N-1]
        LU35 --> LU36[suma = suma + U[i][j] × x[j]]
        LU36 --> LU37[x[i] = y[i] - suma / U[i][i]]
        LU37 --> LU_End([Fin Factorización LU])
    end

    style GS_Start fill:#90EE90
    style GS_End fill:#FFB6C1
    style LU_Start fill:#90EE90
    style LU_End fill:#FFB6C1
    style GS16 fill:#FFE4B5
    style GS18 fill:#FFE4B5
    style LU14 fill:#E6E6FA
    style LU23 fill:#E6E6FA